<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tom.otgstore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
            android:label="@string/signup"
            />
        <activity android:name=".BalanceActivity"
            android:label="@string/balance"
            />
        <activity android:name=".TransactionsActivity"

            android:label="@string/transactions"
            />
        <activity android:name=".ProfileActivity"

            android:label="@string/profile"
            />
        <activity android:name=".HistoryAcivity"

            android:label="@string/history"
            />
    </application>

</manifest>